{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "noImplicitAny": false,
        "sourceMap": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "typeRoots": [
      "../node_modules/@types"
